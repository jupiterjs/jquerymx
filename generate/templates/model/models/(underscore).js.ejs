steal('jquery/model', function(){

/**
 * @class <%=name%>
 * @parent index
 * @inherits jQuery.Model
 * Wraps backend <%=underscore%> services.
 */
$.Model('<%=name%>',
/* @Static */
{
	findAll: "/<%= plural %>.json",
  	findOne : "/<%= plural %>/{id}.json",
  	create : "/<%= plural %>.json",
 	update : "/<%= plural %>/{id}.json",
  	destroy : "/<%= plural %>/{id}.json"
},
/* @Prototype */
{});

})